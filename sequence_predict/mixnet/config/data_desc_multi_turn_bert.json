{
  "input": {
    "std_id": {
      "index": 1,
      "type": "label",
      "mask": "-1"
    },
    "neg": {
      "index": 2,
      "label": "std_id",
      "type": "pos"
    },
    "query": {
      "index": 3,
      "type": "multi_turn_text",
      "length": 200,
      "seg_type": "char",
      "ngram": 1,
      "remove_stopwords": false,
      "embedding_dim": 256,
      "min_count": 5,
      "ner": false
    },
    "query_msa_bert": {
      "index": 3,
      "type": "bert",
      "cache_dir": "./temp/bert-base-chinese",
      "do_lower_case": true,
      "model_dir": "./bert-base-chinese"
    },
    "factor": {
      "index": 4,
      "type": "value",
      "valid_key_file": "factor_valid_keys.txt",
      "min_count": 30
    },
    "route": {
      "index": 5,
      "type": "seq",
      "length": 100,
      "embedding_dim": 50,
      "min_count": 40
    },
    "service": {
      "index": 6,
      "type": "seq",
      "length": 50,
      "embedding_dim": 50,
      "min_count": 40
    }
  },
  "net": {
    "query_msa_shared": {
      "input": "query",
      "output": "query_msa_output_shared",
      "type": "mask_self_attn",
      "params": {
        "inputs": {
          "cnn": {
            "dropout_rate": 0.2,
            "filter_sizes": [5],
            "input_channels": 1,
            "num_filters": 32
          },
          "rnn": {
            "bidirectional": true,
            "dropout_rate": 0.5,
            "hidden_size": 256,
            "num_layers": 1,
            "pooling_type": "max",
            "rnn_type": "gru"
          },
          "bert": {
            "cache_dir": "./temp/bert-base-chinese",
            "do_lower_case": true,
            "model_dir": "./bert-base-chinese"
          }
        },
        "dropout_rate": 0.5
      }
    },
    "query_attn_shared": {
      "input": "query",
      "output": "query_att_output_shared",
      "type": "attention",
      "params": {
        "bidirectional": true,
        "dropout_rate": 0.5,
        "hidden_size": 256,
        "num_layers": 1,
        "pooling_type": "max",
        "rnn_type": "lstm",
        "u_dim": 256
      }
    },
    "factor_dcn_shared": {
      "input": "factor",
      "output": "factor_dcn_output_shared",
      "type": "dcn",
      "params": {
        "cross_net_layers": 4,
        "dnn_params": {
          "dropout_rate": 0.5,
          "layers": [1024, 512, 256],
          "leaky_relu_neg_slope": 0.01
        },
        "use_cross_net": true,
        "use_dnn": true
      }
    },
    "route_lstm_shared": {
      "input": "route",
      "output": "route_lstm_output_shared",
      "type": "rnn",
      "params": {
        "bidirectional": true,
        "dropout_rate": 0.5,
        "hidden_size": 256,
        "num_layers": 1,
        "pooling_type": "max",
        "rnn_type": "lstm"
      }
    },
    "service_lstm_shared": {
      "input": "service",
      "output": "service_lstm_output_shared",
      "type": "rnn",
      "params": {
        "bidirectional": true,
        "dropout_rate": 0.5,
        "hidden_size": 256,
        "num_layers": 1,
        "pooling_type": "max",
        "rnn_type": "lstm"
      }
    }
  },

  "output": {
    "std_id": {
      "input": [],
      "loss": "cross_entropy",
      "neg_loss": "l2",
      "neg_loss_weight": 1,
      "weight": 1
    }
  }
}

