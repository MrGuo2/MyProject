      ],
      "loss": "cross_entropy",
      "neg_loss": "l2",
      "neg_loss_weight": 0.1,
      "weight": 1
    },
    "std_group": {
      "input": [
        "query_cnn_output_shared",
        "query_cnn_output_std_group",
        "factor_dcn_output_shared",
        "route_lstm_output_shared",
        "service_lstm_output_shared"
      ],
      "loss": "cross_entropy",
      "neg_loss": "l2",
      "neg_loss_weight": 0.1,
      "weight": 1
    }
  }
