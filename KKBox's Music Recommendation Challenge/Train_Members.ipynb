   "source": [
    "# 读取train数据集\n",
    "dpath = \"../Data/\"\n",
    "trainTemp = pd.read_csv(dpath+\"train.csv\", iterator=True)\n",
    "# 设置分块参数\n",
    "loop = True\n",
    "chunkSize = 100000\n",
    "chunks = []\n",
    "\n",
    "while loop:\n",
    "    try:\n",
    "        chunk = trainTemp.get_chunk(chunkSize)\n",
    "        chunks.append(chunk)\n",
    "    except StopIteration:\n",
    "        loop = False\n",
    "        print(\"Iteration is stopped.\")\n",
    "train = pd.concat(chunks, ignore_index=True)\n",
    "print(len(train))"
   ]
  },
